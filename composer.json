{
    "name": "muhaiminshihab/ctaroutes",
    "description": "CtaRoutes is a Laravel package that provides a set of convenient routes for common Artisan commands.",
    "type": "library",
    "license": "MIT",
    "version": "1.0.2",
    "keywords": [
        "laravel", "laravel-framework", "laravel-package", "composer-package", 
        "laravel-routes", "cta-routes", "laravel-artisan-commands"
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "MuhaiminShihab\\CtaRoutes\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MuhaiminShihab\\CtaRoutes\\CtaRoutesServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Muhaimin Shihab",
            "email": "hostforshihab@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
